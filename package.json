{
  "name": "calendar-booking-system",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Marius Lungu",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "npm run clean && mkdir dist && npm run copy-files",
    "dev:compile": "tsc && node ./dist/index.js",
    "dev:watch": "nodemon -e ts --exec \"npm run dev:compile\"",
    "dev": "npm run build && npm run dev:watch",
    "prod:build": "tsc",
    "prod": "npm run build && node ./dist/index.js",
    "test": "jest",
    "migrate:run": "knex --knexfile src/knexfile.ts",
    "migrate:create-db": "npm run migrate:run migrate:up 20230511112803_create-calendar-tables.ts --debug",
    "migrate:delete-db": "npm run migrate:run migrate:down 20230511112803_create-calendar-tables.ts --debug",
    "docker:dev": "docker compose -f docker-files/docker-compose.yml -f docker-files/docker-compose.dev.yml -p calendar-app up -d --build",
    "docker:prod": "npm run prod:build && docker compose -f docker-files/docker-compose.yml -f docker-files/docker-compose.prod.yml -p calendar-app up -d --build",
    "docker:dev:down": "docker compose -f docker-files/docker-compose.yml -f docker-files/docker-compose.dev.yml -p calendar-app down",
    "docker:prod:down": "docker compose -f docker-files/docker-compose.yml -f docker-files/docker-compose.prod.yml -p calendar-app down"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.8",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bullmq": "^3.13.4",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "googleapis": "^118.0.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.1",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "nodemailer": "^6.9.2",
    "pg": "^8.10.0",
    "rimraf": "^5.0.1",
    "stripe": "^12.5.0",
    "ts-jest": "^29.1.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
